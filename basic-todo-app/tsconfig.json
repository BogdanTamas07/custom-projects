{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
  },
  /*
   "paths": {
    "@/components/*": [
      "components/*"
    ],
    "@/constants/*": [
      "constants/*"
    ],
    "@/utils/*": [
      "utils/*"
    ],
    "@/styles/*": [
      "styles/*"
    ],
    "@/types/*": [
      "types/*"
    ],
   },
   */
  "include": [
    "./src"
  ],
  "exclude": [
    "node_modules"
  ]
}